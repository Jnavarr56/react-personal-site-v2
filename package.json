{
	"name": "react-personal-site-v2",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@material-ui/core": "^4.9.13",
		"@testing-library/jest-dom": "^4.2.4",
		"@testing-library/react": "^9.3.2",
		"@testing-library/user-event": "^7.1.2",
		"clsx": "^1.1.0",
		"i": "^0.3.6",
		"install": "^0.13.0",
		"node-sass": "^4.14.0",
		"npm": "^6.14.5",
		"react": "^16.12.0",
		"react-awesome-slider": "^4.1.0",
		"react-cookie": "^4.0.3",
		"react-dom": "^16.12.0",
		"react-grid-system": "^6.0.7",
		"react-helmet": "^5.2.1",
		"react-icons": "^3.9.0",
		"react-minimal-pie-chart": "^6.0.1",
		"react-ripples": "^2.2.1",
		"react-router-dom": "^5.1.2",
		"react-scripts": "3.3.0",
		"react-scroll": "^1.7.16",
		"react-spring": "^8.0.27",
		"react-swipeable-views": "^0.13.7",
		"react-tilt": "^0.1.4",
		"react-waypoint": "^9.0.2",
		"reset-css": "^5.0.1",
		"styled-components": "^5.0.0",
		"styled-components-breakpoint": "^3.0.0-preview.20",
		"styled-react-modal": "^2.0.1"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"prettier": "prettier --write $npm_package_config_fixables",
		"eslint": "eslint --fix $npm_package_config_fixables"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"lint-staged": {
		"src/**/*.{js,jsx,json}": [
			"prettier --write",
			"eslint --fix",
			"git add"
		],
		"public/**/*.{js,jsx,json}": [
			"prettier --write",
			"eslint --fix",
			"git add"
		],
		"*.{js,jsx,json}": [
			"prettier --write",
			"eslint --fix",
			"git add"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"config": {
		"fixables": "*.*(json|js|jsx) src/**/*.*(json|js|jsx)"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"eslint-config-prettier": "^6.5.0",
		"eslint-plugin-json": "^2.0.1",
		"husky": "^3.0.9",
		"lint-staged": "^9.4.2",
		"prettier": "^1.18.2"
	}
}
